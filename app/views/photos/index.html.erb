<h1>Listing Photos</h1>

<table class ="catalog">
<% @photos.each do |photo| %>

<tr>
   
    

      <div class ="event">
         <%= photo.event %>
      </div>   
  
      <div class ="photo.description">
         <%= photo.description %>
      </div>
      
      <div class ="photo.price">
         <%= photo.price %>
      </div>
      
      <div class ="photo.image_url">
          <% photo.image_url %>
      </div>
      
      <div class ="photo.image_ref">
        <%= photo.image_ref %>
      </div>
      
      <div class ="links">
        <%= link_to 'Show', photo %>
        <%= link_to 'Edit', edit_photo_path(photo) %>
        <%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %>
      <td><a href="/cart/<%= photo.id %>">Add to Cart</a></td>
     </div>
    <% end %>
 </tr> 
</table>

<br>
<p id="notice"><%= notice %></p>